{
"name": "Pollen", 
"version": "0.1",
"manifest_version": 2,
"description": "stored data..",
"permissions": [
    "activeTab",
    "<all_urls>",
    "tabs"
],

"browser_action": {
    "default_popup": "popout.html"
},

"content_scripts": [
    {
        "matches": ["<all_urls>"],
        "js": ["index.js"]
    }
]
}